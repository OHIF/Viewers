{
  "name": "@ohif/mode-basic",
  "version": "3.12.0-beta.95",
  "description": "A basic mode used to build other modes on top of",
  "author": "OHIF Contributors",
  "license": "MIT",
  "main": "dist/ohif-mode-basic.js",
  "module": "src/index.tsx",
  "files": [
    "dist/**",
    "public/**",
    "README.md"
  ],
  "repository": "OHIF/Viewers",
  "keywords": [
    "ohif-mode"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6",
    "yarn": ">=1.16.0"
  },
  "peerDependencies": {
    "@ohif/core": "3.12.0-beta.95",
    "@ohif/extension-cornerstone": "3.12.0-beta.95",
    "@ohif/extension-cornerstone-dicom-rt": "3.12.0-beta.95",
    "@ohif/extension-cornerstone-dicom-seg": "3.12.0-beta.95",
    "@ohif/extension-cornerstone-dicom-sr": "3.12.0-beta.95",
    "@ohif/extension-default": "3.12.0-beta.95",
    "@ohif/extension-dicom-pdf": "3.12.0-beta.95",
    "@ohif/extension-dicom-video": "3.12.0-beta.95"
  },
  "dependencies": {
    "@babel/runtime": "7.28.2"
  },
  "devDependencies": {
    "webpack": "5.95.0",
    "webpack-merge": "5.10.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config .webpack/webpack.dev.js --watch --output-pathinfo",
    "dev:cornerstone": "yarn run dev",
    "build": "cross-env NODE_ENV=production webpack --config .webpack/webpack.prod.js",
    "build:package": "yarn run build",
    "start": "yarn run dev",
    "test:unit": "jest --watchAll",
    "test:unit:ci": "jest --ci --runInBand --collectCoverage --passWithNoTests"
  }
}
