    "prettier": "^1.18.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-i18next": "^10.11.0",
    "react-redux": "^7.1.0",
    "react-viewerbase": "^0.15.1",
    "redux": "^4.0.1",
    "shader-loader": "^1.3.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylus": "^0.54.5",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "worker-loader": "^2.0.0"
