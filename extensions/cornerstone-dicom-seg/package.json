    "@ohif/i18n": "3.12.0-beta.127",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "12.3.1",
    "react-router": "6.30.3",
    "react-router-dom": "6.30.3"
  },
  "dependencies": {
    "@babel/runtime": "7.28.2",
    "@cornerstonejs/adapters": "4.15.9",
