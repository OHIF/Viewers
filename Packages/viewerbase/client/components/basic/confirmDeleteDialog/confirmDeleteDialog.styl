#confirmDeleteDialog
  display: none
  position: absolute
  top: 0
  bottom: 0
  left: 0
  right: 0
  z-index: 100
  width: 300px
  min-height: 145px
  max-height: 300px
  height: fit-content
  margin: auto
  border-radius: 5px
  padding: 10px 20px 10px 20px
  background-color: rgba(255,255,255,1)
  outline: none

  .btn
    text-decoration: none

  #cancel
    float: left

  #confirm
    float: right