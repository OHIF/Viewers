.textEntryDialog
  display: none
  position: absolute
  top: 0
  bottom: 0
  left: 0
  right: 0
  z-index: 100
  width: 50%
  max-width: 400px
  height: 170px
  margin: auto
  border-radius: 5px
  padding: 10px 20px 10px 20px
  background-color: rgba(255,255,255,1)
  outline: none

  .dialogContent
    margin-bottom: 10px

  .cancel
    float: left

  .save
    float: right

  input.currentValue
    width: 95%
    text-align: left